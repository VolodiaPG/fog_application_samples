{
    "server": {
        "host": "127.0.0.1:9003"
    },

    "routes": {
        "/": "out/"
    },
    "static": {
      "directory": "out",
      "served_from": "/"
    },
    "config": {
        "enable_hot_reload": false,
        "fast_mem_cache": true,
        "enable_cache_control": true,
        "enable_directory_listing": true,
        "minify_html": true,
        "follow_symlinks": false,
        "enable_logging": false
    },

    "insert_headers": {
        "x-greetings": "I'm a goblin!"
    }
}
